<script>
    function toggleTextDecoration(index) {
        var text = document.getElementById("text_" + index);
        var checkbox = document.getElementById('checkbox_' + index);
        if (checkbox.checked) {
            text.style.textDecoration = "line-through";

        } else {
            text.style.textDecoration = "none";
        }
    }
</script>
